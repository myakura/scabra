{
  "manifest_version": 2,
  "name": "Scabra",
  "description": "Open multiple URLs at once.",
  "version": "0.9.0",
  "browser_action": {
    "default_title": "Open URLs",
    "default_icon": "icon.png",
    "default_popup": "scabra_popup.html"
  },
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Alt+Shift+O"
			}
		}
	}
}
