{
	"manifest_version": 3,
	"name": "Scabra",
	"description": "Open URLs at once.",
	"version": "0.11.0",
	"permissions": ["activeTab"],
	"icons": {
		"48": "icons/icon_gray.png",
		"96": "icons/icon_gray.png"
	},
	"background": {
		"scripts": ["scabra_background.js"],
		"service_worker": "scabra_background.js"
	},
	"action": {
		"default_title": "Open URLs",
		"default_icon": "icons/icon_lightgray.png",
		"default_popup": "scabra_popup.html"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Alt+Shift+O"
			}
		}
	}
}
