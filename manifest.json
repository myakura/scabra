{
	"manifest_version": 3,
	"name": "Scabra",
	"description": "Open URLs at once.",
	"version": "0.12.3",
	"permissions": ["activeTab"],
	"icons": {
		"48": "icons/icon_gray.png",
		"96": "icons/icon_gray.png"
	},
	"theme_icons": [
		{
			"light": "icons/icon_white.png",
			"dark": "icons/icon_black.png",
			"size": 96
		}
	],
	"action": {
		"default_title": "Open URLs",
		"default_icon": "icons/icon_lightgray.png",
		"default_popup": "scabra_popup.html",
		"theme_icons": [
			{
				"light": "icons/icon_white.png",
				"dark": "icons/icon_black.png",
				"size": 32
			}
		]
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Alt+Shift+O"
			}
		}
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{35c8b747-cc80-49af-87e5-ae38175d4de0}"
		}
	}
}
